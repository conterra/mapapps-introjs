{
  "Bundle-SymbolicName": "dn_introjs",
  "Bundle-Version": "1.0.0",
  "Bundle-Name": "${bundleName}",
  "Bundle-Description": "${bundleDescription}",
  "Bundle-Vendor": "con terra GmbH",
  "Product-Name": "con terra Developer Network",
  "Bundle-StartLevel": 51,
  "Config-Bundle-Location": "/config",
  "Require-Bundle": [
  ],
  "CSS-Themes-Extension": [
    {
      "name": "*",
      "files": [
        "./styles/introjs.css"
      ]
    }
  ],
  "Components": [
    {
      "name": "UserTutorial",
      "propertiesConstructor": true,
      "immediate": true,
      "provides": [
        "ct.framework.api.EventHandler",
        "dn_introjs.UserTutorial"
      ],
      "properties": {
        "showStepNumbers": false,
        "showBullets": true,
        "showProgress": false,
        "showButtons": true,
        "keyboardNavigation": true,
        "exitOnEsc": true,
        "exitOnOverlayClick": false,
        "nextLabel": "${intro.next}",
        "prevLabel": "${intro.back}",
        "skipLabel": "${intro.skip}",
        "doneLabel": "${intro.done}",
        "steps": []
      },
      "references": [
        {
          "name": "_eventService",
          "providing": "ct.framework.api.EventService"
        },
        {
          "name": "_esriMap",
          "providing": "esri.Map"
        }
      ]
    }
  ]
}
